{
    "name": "imarc/craft-pickture",
    "description": "A custom field type to let users pick between thumbnails instead of just text labels.",
    "type": "craft-plugin",
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^3.7 || ^4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Imarc\\Pickture\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Imarc",
            "homepage": "https://www.imarc.com"
        }
    ],
    "support": {
        "issues": "https://github.com/imarc/craft-pickture/issues",
        "source": "https://github.com/imarc/craft-pickture/issues",
        "docs": "https://github.com/imarc/craft-pickture/issues"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "extra": {
        "name": "Pickture",
        "handle": "pickture"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
